<columnMap xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="../xml/caTissueTextMap.xsd"
 headerRow="true"
 fileType="xlText">
    <dateFormat>MM/dd/yyyy</dateFormat>
    <rowClass>SpecimenCollectionGroup</rowClass>
    <rowClass>Specimen</rowClass>
    <column attributeName="protocolTitle" fixedValue="Demo of collection protocol"/>
    <column attributeName="PPI" columnNo="1"/>
    <column attributeName="calendarPoint" columnNo="2"/>
    <column attributeName="siteName" fixedValue="Haven outpatients clinic"/>
    <column attributeName="SCGName" columnNo="3"/>
    <column attributeName="clinicalStatus" columnNo="4"/>
    <column attributeName="clinicalDiagnosis" columnNo="5"/>
    <column attributeName="userEmail" fixedValue="bill@rfu.com"/>
    <column attributeName="groupCollTime" columnNo="7"/>
    <column attributeName="groupRecTime" columnNo="8"/>
    <column attributeName="specCollTime" columnNo="7"/>
    <column attributeName="specRecTime" columnNo="8"/>
    <column attributeName="label" columnNo="9"/>
    <column attributeName="pathologicalStatus" columnNo="10"/>
    <column attributeName="tissueSite" columnNo="11"/>
    <column attributeName="quantity" columnNo="12"/>
    <column attributeName="availableQuantity" columnNo="12"/>
    <column attributeName="lineage" columnNo="13"/>
    <column attributeName="specimenType" columnNo="14"/>
    <column attributeName="tissueSide" columnNo="15"/>
    <column attributeName="collectionProcedure" fixedValue="Not Specified"/>
    <column attributeName="container" columnNo="16">
        <valueMapping>
            <valueMap fromValue="Red Top" ignoreCase="true" toValue="EDTA Vacutainer"/>
            <valueMap fromValue="Green Top" ignoreCase="true" toValue="Serum Separator Vacutainer"/>
            <valueMap fromValue="Blue Top" ignoreCase="true" toValue="Citrate Vacutainer"/>
        </valueMapping>
    </column>
    <column attributeName="receivedQuality" fixedValue="Not Specified"/>
    <column attributeName="groupActivityStatus" fixedValue="Active"/>
    <column attributeName="specimenActivityStatus" fixedValue="Active"/>
    <column attributeName="specimenAvailable" fixedValue="true"/>
    <column attributeName="groupCollectionStatus" fixedValue="Complete"/>
    <column attributeName="specimenCollectionStatus" fixedValue="Collected"/>
    
</columnMap>
