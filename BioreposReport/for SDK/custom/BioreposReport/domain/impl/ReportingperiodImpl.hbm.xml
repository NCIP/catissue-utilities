<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain.impl">
	<class name="ReportingperiodImpl" table="reporting_period" lazy="true">

		<id name="id" column="id">
			<generator class="assigned" />
		</id>

		<property name="start" column="start" type="date" />

		<property name="end" column="end" type="date" />

		<set name="numberShipped" inverse="true" lazy="true">
			<key column="report_period_id" />
			<one-to-many class="NumberShippedImpl" />
		</set>
	</class>
</hibernate-mapping>